package cn.wjj.upgame.common 
{
	/**
	 * 0-255 之间的值
	 * @author GaGa
	 */
	public class UpInfoDisplayType 
	{
		public function UpInfoDisplayType() { }
		/** 无图 **/
		public static const no:uint = 0;
		/** U2格式类型 **/
		public static const u2:uint = 1;
		/** PNG的图片格式 **/
		public static const png:uint = 2;
		/** JPG的图片格式 **/
		public static const jpg:uint = 3;
		/** 位图类型 **/
		public static const bitmap:uint = 4;
	}
}